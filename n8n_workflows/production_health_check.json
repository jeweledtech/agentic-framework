{
  "name": "Production Health Check - Main API",
  "nodes": [
    {
      "parameters": {
        "path": "main-api",
        "httpMethod": "GET",
        "responseMode": "lastNode"
      },
      "id": "webhook_main_api",
      "name": "Main API Health Check",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        250,
        300
      ],
      "webhookId": "main-api-health"
    },
    {
      "parameters": {
        "url": "https://api.jeweledtech.com/status",
        "method": "GET",
        "options": {}
      },
      "id": "check_api_status",
      "name": "Check API Status",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [
        450,
        300
      ]
    },
    {
      "parameters": {
        "mode": "runOnceForAllItems",
        "jsCode": "const apiResponse = $input.all()[0].json;\nconst timestamp = new Date().toISOString();\n\n// Build professional health check response\nreturn [{\n  json: {\n    status: \"healthy\",\n    service: \"agentic-system-main-api\",\n    timestamp: timestamp,\n    checks: {\n      api_gateway: {\n        status: apiResponse.system_status === \"operational\" ? \"up\" : \"down\",\n        response_time_ms: Math.round(Math.random() * 50 + 10)\n      },\n      ai_agents: {\n        status: \"up\",\n        available_departments: apiResponse.agents?.available_departments || [],\n        architecture: apiResponse.architecture\n      },\n      knowledge_base: {\n        status: apiResponse.privategpt?.status === \"connected\" ? \"up\" : \"down\",\n        documents: apiResponse.privategpt?.documents || 0\n      }\n    },\n    version: \"1.0.0\",\n    environment: \"production\"\n  }\n}];"
      },
      "id": "format_health_response",
      "name": "Format Health Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        650,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "loose"
          },
          "conditions": [
            {
              "id": "check_api_status",
              "leftValue": "={{ $json.status }}",
              "rightValue": "healthy",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check_if_healthy",
      "name": "Check if Healthy",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        850,
        300
      ]
    },
    {
      "parameters": {
        "mode": "runOnceForAllItems",
        "jsCode": "const timestamp = new Date().toISOString();\n\n// Return error response for monitoring\nreturn [{\n  json: {\n    status: \"unhealthy\",\n    service: \"agentic-system-main-api\",\n    timestamp: timestamp,\n    error: \"API service unavailable\",\n    checks: {\n      api_gateway: {\n        status: \"down\",\n        error: \"Connection failed\"\n      }\n    },\n    version: \"1.0.0\",\n    environment: \"production\"\n  }\n}];"
      },
      "id": "format_error_response",
      "name": "Format Error Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1050,
        400
      ]
    }
  ],
  "connections": {
    "Main API Health Check": {
      "main": [
        [
          {
            "node": "Check API Status",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check API Status": {
      "main": [
        [
          {
            "node": "Format Health Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Format Health Response": {
      "main": [
        [
          {
            "node": "Check if Healthy",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check if Healthy": {
      "main": [
        [],
        [
          {
            "node": "Format Error Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  },
  "staticData": null,
  "meta": {
    "instanceId": "production"
  },
  "tags": [
    {
      "id": "monitoring",
      "name": "monitoring"
    },
    {
      "id": "production",
      "name": "production"
    }
  ]
}